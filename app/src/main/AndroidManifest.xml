<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.crew">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".activity.NoticeActivity"></activity>
        <activity android:name=".activity.MyPageActivity" />
        <activity android:name=".activity.GroupJoinActivity" />
        <activity android:name=".activity.GroupActivity" />
        <activity android:name=".activity.CreateActivity" />
        <activity android:name=".activity.SearchActivity" />
        <activity android:name=".activity.MainActivity" />
        <activity android:name=".preLogin.IntroActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <action android:name="android.intent.action.SEARCH" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

            <meta-data
                android:name="android.app.searchable"
                android:resource="@xml/searchable" />
        </activity>
        <activity android:name=".preLogin.logInActivity" />
        <activity android:name=".preLogin.signUpActivity" />
        <activity android:name=".preLogin.pwdResetActivity" />
        <activity android:name=".activity.memberInitActivity" />
    </application>

</manifest>